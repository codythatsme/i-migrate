{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate-routes": "bunx tsr generate",
    "watch-routes": "bunx tsr watch",
    "dev": "bunx tsr watch & bun --hot src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "build": "bun run generate-routes && bun run scripts/build.ts",
    "build:windows": "bun run generate-routes && bun run scripts/build.ts --compile --target=bun-windows-x64 --outfile=dist/i-migrate.exe",
    "build:windows:signed": "bun run build:windows && bun run scripts/sign-windows.ts",
    "build:mac": "bun run generate-routes && bun run scripts/build.ts --compile --target=bun-darwin-arm64 --outfile=dist/i-migrate",
    "build:mac:signed": "bun run build:mac && bun run scripts/sign-mac.ts",
    "sign:mac": "bun run scripts/sign-mac.ts",
    "sign:windows": "bun run scripts/sign-windows.ts",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:push": "bunx drizzle-kit push",
    "db:studio": "bunx drizzle-kit studio",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt src .vscode *.ts *.json",
    "format:check": "oxfmt --check src .vscode *.ts *.json",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@effect/platform": "^0.94.0",
    "@effect/platform-browser": "^0.74.0",
    "@effect/platform-bun": "^0.87.0",
    "@effect/rpc": "^0.73.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.15",
    "@tanstack/react-router": "^1.144.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "effect": "^3.19.13",
    "lucide-react": "^0.562.0",
    "nuqs": "^2.8.6",
    "radix-ui": "^1.4.3",
    "react": "^19",
    "react-dom": "^19",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tanstack/router-cli": "^1.145.1",
    "@types/babel__core": "^7.20.5",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "better-sqlite3": "^12.5.0",
    "drizzle-kit": "^0.31.8",
    "oxfmt": "^0.21.0",
    "oxlint": "^1.36.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0"
  }
}
